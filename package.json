{
  "name": "universal-redux-starter",
  "version": "0.0.1",
  "description": "Minimal starter for using universal-redux npm package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bdefore/universal-redux-starter.git"
  },
  "scripts": {
    "build": "rook-build",
    "dev-server": "NODE_CONFIG_DIR=./config/server NODE_PATH=./src nodemon --watch src --watch config -e js,json,json5 ./node_modules/.bin/rook-server",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run dev-server\"",
    "lint": "./node_modules/.bin/eslint -c .eslintrc src",
    "server": "NODE_CONFIG_DIR=./config/server NODE_PATH=./src NODE_ENV=production ./node_modules/.bin/rook-server",
    "test": "NODE_CONFIG_DIR=./config/server NODE_ENV=test mocha --colors --compilers js:babel-core/register --require ./node_modules/rook/bin/test.js --recursive ./src/**/*.test.js",
    "watch-client": "rook-watch"
  },
  "author": {
    "name": "Buck DeFore"
  },
  "license": "MIT",
  "dependencies": {
    "hapi-auth-cookie": "^3.1.0",
    "highlight.js": "^9.0.0",
    "js-yaml": "^3.5.2",
    "kerberos": "^0.0",
    "lodash": "^3.10.1",
    "marked": "^0.3.5",
    "moment": "^2.10.6",
    "promised-mongo": "^1.2.0",
    "react-ace": "^3.0.0",
    "react-ga": "^1.2.0",
    "rook": "file:///Users/Andre/GitHub/rook"
  },
  "devDependencies": {
    "concurrently": "1.0.0",
    "hapi": "^11.1.2",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "4.2.1",
    "react-router": "2.0.0-rc5",
    "react-router-redux": "3.0.0"
  }
}
